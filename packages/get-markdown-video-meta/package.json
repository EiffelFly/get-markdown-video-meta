{
  "name": "get-markdown-video-meta",
  "version": "0.3.0",
  "description": "Get your video's metadata in the markdown or mdx file. Powered by remarkjs, fully typed.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "node --loader tsx --test **/*.test.ts",
    "start": "node --loader tsx ./src/example.ts",
    "ts-types": "tsc --emitDeclarationOnly --outDir build",
    "build": "pnpm rimraf build && tsup ./src"
  },
  "keywords": [
    "markdown",
    "MDX",
    "remark",
    "SEO",
    "javascript",
    "typescript"
  ],
  "author": "EiffelFly",
  "license": "MIT",
  "devDependencies": {
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.11.18",
    "rimraf": "^4.1.2",
    "tsup": "^7.0.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.5",
    "unified": "^10.1.2"
  },
  "dependencies": {
    "fast-glob": "^3.2.12",
    "remark": "^14.0.2",
    "remark-directive": "^2.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-mdx": "^2.2.1",
    "unist-util-visit": "^4.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
